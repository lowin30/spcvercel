export default async function ContactoDetailPage({ params }: { params: { id: string } }) {
  // Resolver los parámetros antes de usarlos (solución según guia-errores.md)
  const resolvedParams = await params;
  const contactoId = resolvedParams.id;

  return (
    <div style={{ padding: '20px' }}>
      <h1>Detalle de Contacto</h1>
      <p>ID del contacto: {contactoId}</p>
      <p>Esta página ha sido simplificada para resolver problemas de compilación en Vercel.</p>
    </div>
  );
}
